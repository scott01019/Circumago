    // Glossary Activity
    <string name="glossary_title_text">Glossary</string>
    <string name="glossary_circumago_text">Circumago:</string>
    <string name="glossary_circumago_definition_text">
        a Latin word meaning \"I make rotate\"; also the name of this app!
    </string>
    <string name="glossary_node_text">Node:</string>
    <string name="glossary_node_definition_text">
        any positioned piece on a Circumago puzzle board (e.g. edges, corners, and centers)
    </string>
    <string name="glossary_orientable_text">Orientable:</string>
    <string name="glossary_orientable_definition_text">
        a puzzle node which has a manipulable orientation but does not change position
    </string>
    <string name="glossary_permutable_text">Permutable:</string>
    <string name="glossary_permutable_definition_text">
        a puzzle node which has a manipulable position but does not change orientation
    </string>
    <string name="glossary_rotatable_text">Rotatable:</string>
    <string name="glossary_rotatable_definition_text">
        a puzzle node which has both a manipulable position and orientation
    </string>

